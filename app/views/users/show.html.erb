<%= render 'list', user: @user %>
<%= render 'books/list', book: @book %>

<% @books.each do |book| %>
<div>
  <%= link_to books_path(book.id) do %>
    <%= image_tag @user.get_profile_image(50,50) %>
  <% end %>
  <%= link_to book_path(book.id) do %>
    <p><%= book.title %></p>
  <% end %>
  <p><%= book.body %></p>
</div>
<% end %>
