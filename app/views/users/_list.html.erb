<div>
  <h2>User info</h2>
  <%= image_tag user.get_profile_image(100,100) %>
  <table class="table table-striped">
  <p>Name <%= user.name %></p>
  <p>Intorduction <%= user.introduction %></p>
  </table>
  <% if user = current_user %>
  <p><%= link_to "Update User", edit_user_path(user) %></p>
  <% end %>
</div>